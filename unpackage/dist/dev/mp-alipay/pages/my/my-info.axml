<block a:if="{{userInfo}}"><view class="my-info data-v-431813f6"><view data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" class="line flex a-center j-between flex-row data-v-431813f6" onTap="__e"><view class="left data-v-431813f6">个人头像</view><view class="right flex a-center j-end flex-row data-v-431813f6"><image class="avatar data-v-431813f6" src="{{userInfo.avatarUrl}}"></image><image class="icon data-v-431813f6" src="../../static/image/show1.png"></image></view></view><view data-event-opts="{{[['tap',[['editPage',['$0','nickName'],['userInfo.nickName']]]]]}}" class="line flex a-center j-between flex-row data-v-431813f6" onTap="__e"><view class="left data-v-431813f6">姓名</view><view class="right flex a-center j-end flex-row data-v-431813f6"><text class="user-name data-v-431813f6">{{userInfo.nickName}}</text><image class="icon data-v-431813f6" src="../../static/image/show1.png"></image></view></view><view class="line flex a-center j-between flex-row data-v-431813f6"><view class="left data-v-431813f6">性别</view><view class="right flex a-center j-end flex-row data-v-431813f6"><view class="flex a-center j-end flex-row data-v-431813f6"><text data-event-opts="{{[['tap',[['sexValChange',[1]]]]]}}" class="{{(('checkBox data-v-431813f6')+' '+(sexVal===1&&'isChecked'))}}" onTap="__e"></text>男<text data-event-opts="{{[['tap',[['sexValChange',[2]]]]]}}" class="{{(('checkBox checkBox1 data-v-431813f6')+' '+(sexVal===2&&'isChecked'))}}" onTap="__e"></text>女</view><image class="icon data-v-431813f6" src="../../static/image/show1.png"></image></view></view><view class="line flex a-center j-between flex-row data-v-431813f6"><view class="left data-v-431813f6">手机号</view><view hidden="{{!(phoneButton)}}" class=" right flex a-center j-end flex-row data-v-431813f6"><button class="btns data-v-431813f6" open-type="getAuthorize" scope="phoneNumber" data-event-opts="{{[['getAuthorize',[['$onAliGetAuthorize',['getPhoneNumber','$event']]]],['error',[['$onAliAuthError',['getPhoneNumber','$event']]]]]}}" onGetAuthorize="__e" onError="__e">获取电话号码</button></view><view hidden="{{!(phoneInput)}}" class="right flex a-center j-end flex-row data-v-431813f6"><text class="phone data-v-431813f6">{{phone}}</text></view></view><view class="line flex a-center j-between flex-row data-v-431813f6"><view class="left data-v-431813f6">出生日期</view><view class="right flex a-center j-end flex-row data-v-431813f6"><picker mode="date" value="{{userInfo.birthday}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" onChange="__e" class="data-v-431813f6"><view class="uni-input data-v-431813f6">{{userInfo.birthday||'2020-07-10'}}</view></picker><image class="icon data-v-431813f6" src="../../static/image/show1.png"></image></view></view></view></block>